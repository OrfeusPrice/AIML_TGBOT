<aiml>

	<!-- ФУНКЦИИ (без ответа) -->

	<category>
		<pattern>MYNAME *</pattern>
		<template>
			<think>
				<set name="user">
					<star/>
				</set>
			</think>
		</template>
	</category>

	<category>
		<pattern>PLANET *</pattern>
		<template>
			<think>
				<set planet="planet">
